let c = coordinator("demo");
let r = c.route("demo", 42, "hello");

// Note: RIB lists must have a single element type; prefer a record/object when combining mixed results.
let p = {
  name: "abc",
  count: 7,
  note: some("n"),
  flags: ["x", "y", "z"],
  nested: { x: 1.5, tags: ["a", "b"] }
};

let t = c.route-typed("demo", 42, p);

{ r: r, t: t }